{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    {
        "identifier": "fs:default",
        "allow": [
            "read",
            "write",
            "copy",
            "createFile",
            "createDir"
        ]
    }
  ]
}
